{
    "name": "@rotcetihra/utils",
    "version": "0.1.0",
    "type": "module",
    "author": "Arhitector <rotcetihra@mail.ru>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/rotcetihra/js-utils.git"
    },
    "bugs": {
        "url": "https://github.com/rotcetihra/js-utils/issues"
    },
    "homepage": "https://github.com/rotcetihra/js-utils/#readme",
    "engines": {
        "node": ">=18"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs"
        }
    },
    "types": "./dist/index.d.mts",
    "main": "./dist/index.mjs",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "npm run src:build && npm run docs:build",
        "clean": "npm run src:clean && npm run docs:clean",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "src:clean": "rimraf dist",
        "src:build": "npm run src:clean && tsc -p tsconfig.build.json",
        "docs:clean": "rimraf docs",
        "docs:build": "npm run docs:clean && typedoc",
        "docs:serve": "npx http-server docs -o"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.1.2",
        "ts-jest-resolver": "^2.0.1",
        "ts-node": "^10.9.2",
        "typedoc": "^0.28.10",
        "typedoc-github-theme": "^0.3.1",
        "typedoc-plugin-mdn-links": "^5.0.9",
        "typedoc-plugin-rename-defaults": "^0.7.3",
        "typescript": "^5.9.2"
    },
    "packageManager": "npm@9.2.0",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    }
}
